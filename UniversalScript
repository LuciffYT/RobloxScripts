local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local flying = false
local flySpeed = 50 -- Varsayƒ±lan u√ßma hƒ±zƒ±

-- U√ßma fonksiyonu
local function fly()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000) -- Yeterli kuvvet
    bodyVelocity.Parent = humanoidRootPart

    while flying do
        wait()
        -- U√ßma hareketi
        local moveDirection = Vector3.new()

        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + workspace.CurrentCamera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - workspace.CurrentCamera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - workspace.CurrentCamera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + workspace.CurrentCamera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            bodyVelocity.Velocity = Vector3.new(moveDirection.X * flySpeed, flySpeed, moveDirection.Z * flySpeed) -- Yukarƒ± hareket
        elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
            bodyVelocity.Velocity = Vector3.new(moveDirection.X * flySpeed, -flySpeed, moveDirection.Z * flySpeed) -- A≈üaƒüƒ± hareket
        else
            bodyVelocity.Velocity = Vector3.new(moveDirection.X * flySpeed, 0, moveDirection.Z * flySpeed) -- Yatay hareket
        end
    end

    bodyVelocity:Destroy()
end

-- Rayfield men√ºs√ºndeki buton
local Button = MainTab:CreateButton({ 
    Name = "Fly",
    Callback = function()
        flying = not flying -- U√ßma durumunu deƒüi≈ütir
        if flying then
            fly() -- U√ßmayƒ± ba≈ülat
        end
    end,
})

-- U√ßma hƒ±zƒ±nƒ± kontrol etmek i√ßin Slider olu≈üturma
local Slider = MainTab:CreateSlider({
    Name = "Fly Speed",
    Range = {25, 1000}, -- Minimum ve maksimum deƒüerler
    Increment = 5, -- Artƒ±≈ü miktarƒ±
    Suffix = "flyspeed", -- Aray√ºze eklenecek birim
    CurrentValue = flySpeed, -- Ba≈ülangƒ±√ß deƒüeri
    Callback = function(value)
        flySpeed = value -- Slider'dan alƒ±nan deƒüeri flySpeed'e ata
    end,
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- NoClip durumu
local noClip = false

-- NoClip fonksiyonu
local function toggleNoClip()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

    if noClip then
        -- NoClip'i kapat
        for _, part in ipairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    else
        -- NoClip'i a√ß
        for _, part in ipairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
    noClip = not noClip -- Durumu deƒüi≈ütir
end

-- Rayfield men√ºs√ºndeki buton
local Button = MainTab:CreateButton({ 
    Name = "NoClip",
    Callback = function()
        toggleNoClip() -- NoClip fonksiyonunu √ßaƒüƒ±r
    end,
})



local SecondTab = Window:CreateTab("üíÄVisuals", nil) -- Title, Image
local Section = SecondTab:CreateSection("Main")

local Button = SecondTab:CreateButton({
   Name = "Highlight ESP",
   Callback = function()
local Players = game:GetService("Players")

-- ESP Fonksiyonu
local function addESP(player)
    local function onCharacterAdded(character)
        -- Highlight nesnesini olu≈ütur
        local highlight = Instance.new("Highlight")
        highlight.Adornee = character
        highlight.FillColor = Color3.new(1, 1, 1) -- Beyaz renk
        highlight.OutlineColor = Color3.new(1, 1, 1) -- Beyaz outline
        highlight.FillTransparency = 1 -- Sadece outline g√∂ster
        highlight.Parent = character
    end
    
    -- Karakter varsa hemen ekle, yoksa karakter geldiƒüinde ekle
    if player.Character then
        onCharacterAdded(player.Character)
    end
    player.CharacterAdded:Connect(onCharacterAdded)
end

-- T√ºm oyuncular i√ßin ESP ekle
for _, player in pairs(Players:GetPlayers()) do
    addESP(player)
end

-- Yeni katƒ±lan oyuncular i√ßin ESP ekle
Players.PlayerAdded:Connect(addESP)

   end,
})

